configpath: ../../xeol-agent.yaml
presenteropt: 1
output: json
quiet: false
log:
  structured: false
  levelopt: debug
  level: debug
  filelocation: ./xeol-agent.log
clioptions:
  configpath: ../../xeol-agent.yaml
  verbosity: 0
dev:
  profilecpu: false
kubeconfig:
  path: ""
  cluster: docker-desktop
  clustercert: ""
  server: ""
  user:
    userconftype: 0
    userconf: ""
    clientcert: ""
    privatekey: ""
    token: ""
kubernetes:
  requesttimeoutseconds: 60
  requestbatchsize: 100
  workerpoolsize: 100
namespaces: []
kubernetesrequesttimeoutseconds: -1
namespaceselectors:
  include: []
  exclude: []
missingtagpolicy:
  policy: digest
  tag: UNKNOWN
runmode: 0
mode: adhoc
ignorenotrunning: true
pollingintervalminutes: 300
xeoldetails:
  apikey: '******'
  http:
    insecure: false
    timeoutseconds: 10
